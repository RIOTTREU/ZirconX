function RUN() ------- 4 √ó 100 
A = gg.prompt({"Choose a level of movement speed. 1 ~ 1000‚ö°"}, nil, {"Numbers"})
gg.setRanges(gg.REGION_JAVA_HEAP)
gg.searchNumber("2000;1~1000F::65", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("1~1000", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(10, nil, nil, nil, nil, nil, nil, nil, nil, nil)
gg.editAll(A[1], gg.TYPE_FLOAT)
gg.clearResults()
gg.toast("Success")
end

function ATK() ------‡πÇ‡∏à‡∏°‡∏ï‡∏µ
Q = gg.choice({"Open hit fast [Speed ATK]" ,"Open hit leak [GOD ATK]" ,"Open quick arm [MOVABLE]","Turn off both","üéíBACKING"},nil," üöÄAttack function")
if Q == 1 then A1() end
if Q == 2 then A1() gg.sleep(500) A2() end
if Q == 3 then A3()end 
if Q == 4 then A4() end
if Q == 5 then MN2() end
end

function A1()
gg.clearResults()
r = {"4"}
gg.setRanges(gg.REGION_JAVA_HEAP)
gg.searchNumber("2000;1~1000F::65" , gg.TYPE_DWORD)
gg.refineNumber("1~1000", gg.TYPE_FLOAT)
q = gg.getResults(1)

for i = 1,#r do
            p = {}
			p[i] = {}
		    p[i].address = q[1].address + r[i]
			p[i].flags = gg.TYPE_FLOAT
			p[i].value = 0
			p[i].freeze = true
			gg.setValues(p)
			gg.addListItems(p)
	end	
		gg.clearResults()
gg.clearResults()	
gg.toast("Success")
end

function A2()
gg.clearResults()
r = {"-24"}
gg.setRanges(gg.REGION_JAVA_HEAP)
gg.searchNumber("2000;1~1000F::65" , gg.TYPE_DWORD)
gg.refineNumber("1~1000", gg.TYPE_FLOAT)
q = gg.getResults(1)

for i = 1,#r do
            p = {}
			p[i] = {}
		    p[i].address = q[1].address + r[i]
			p[i].flags = gg.TYPE_DWORD
			p[i].value = 0
			p[i].freeze = true
			p[i].freezeType = gg.FREEZE_IN_RANGE
			p[i].freezeFrom = -250
			p[i].freezeTo = 1
			gg.setValues(p)
			gg.addListItems(p)
	end	
		gg.clearResults()
gg.clearResults()	
gg.toast('‚†Ä‚†Ä‚†Ä‚†Ä‚£†‚£∂‚°æ‚†è‚†â‚†ô‚†≥‚¢¶‚°Ä‚†Ä‚†Ä‚†Ä‚¢†‚†û‚†â‚†ô‚†≤‚°Ä‚†Ä\n‚†Ä‚†Ä‚†Ä‚£¥‚†ø‚†è‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢≥‚°Ä‚†Ä‚°è‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢∑\n‚†Ä‚†Ä‚¢†‚£ü‚£ã‚°Ä‚¢Ä‚£Ä‚£Ä‚°Ä‚†Ä‚£Ä‚°Ä‚£ß‚†Ä‚¢∏‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä ‚°á\n‚†Ä‚†Ä‚¢∏‚£Ø‚°≠‚†Å‚†∏‚£õ‚£ü‚†Ü‚°¥‚£ª‚°≤‚£ø‚†Ä‚£∏‚†Ä‚†ÄOK‚†Ä ‚°á\n‚†Ä‚†Ä‚£ü‚£ø‚°≠‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢±‚†Ä‚†Ä‚£ø‚†Ä‚¢π‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä ‚°á\n‚†Ä‚†Ä‚†ô‚¢ø‚£Ø‚†Ñ‚†Ä‚†Ä‚†Ä‚¢Ä‚°Ä‚†Ä‚†Ä‚°ø‚†Ä‚†Ä‚°á‚†Ä‚†Ä‚†Ä‚†Ä‚°º\n‚†Ä‚†Ä‚†Ä‚†Ä‚†π‚£∂‚†Ü‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚°¥‚†É‚†Ä‚†Ä‚†ò‚†§‚£Ñ‚£†‚†û‚†Ä\n‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢∏‚£∑‚°¶‚¢§‚°§‚¢§‚£û‚£Å‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä\n‚†Ä‚†Ä‚¢Ä‚£§‚£¥‚£ø‚£è‚†Å‚†Ä‚†Ä‚†∏‚£è‚¢Ø‚£∑‚£ñ‚£¶‚°Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä\n‚¢Ä‚£æ‚£Ω‚£ø‚£ø‚£ø‚£ø‚†õ‚¢≤‚£∂‚£æ‚¢â‚°∑‚£ø‚£ø‚†µ‚£ø‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä\n‚£º‚£ø‚†ç‚†â‚£ø‚°≠‚†â‚†ô‚¢∫‚£á‚£º‚°è‚†Ä‚†Ä‚†Ä‚£Ñ‚¢∏‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä\n‚£ø‚£ø‚£ß‚£Ä‚£ø‚Ä¶‚Ä¶‚Ä¶‚£Ä‚£∞‚£è‚£ò‚£Ü‚£Ä') 
