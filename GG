gg.refineNumber("0",gg.TYPE_DWORD)
t = gg.getResults(10)
for i, v in ipairs(t) do
	if v.flags == gg.TYPE_DWORD then
		v.value = "6"
		v.freeze = true
	end
	end
gg.addListItems(t) 
t = nil
gg.clearResults()
gg.toast("done")
end


