gg.editAll("60", gg.TYPE_FLOAT)
local o = gg.getResults(100,nil,nil,nil,156.0,156.0,gg.TYPE_FLOAT)
gg.editAll("60",gg.TYPE_FLOAT)
gg.clearResults()
end

