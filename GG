q = gg.getResults(1)

for i = 1,#r do
            p = {}
			p[i] = {}
		    p[i].address = q[1].address + r[i]
			p[i].flags = 4 
			p[i].value = 1
			p[i].freeze = false
			gg.setValues(p)
	end
gg.clearResults()
gg.toast("Success")
end

