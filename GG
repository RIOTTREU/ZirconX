end

function LLL2() -- ‡πÅ‡∏°‡∏û
gg.clearResults()
r = {"-104"}
gg.setRanges(gg.REGION_JAVA_HEAP)
gg.searchNumber("20008" , gg.TYPE_DWORD)
gg.refineNumber("20008", gg.TYPE_DWORD)
q = gg.getResults(1)

for i = 1,#r do
            p = {}
			p[i] = {}
		    p[i].address = q[1].address + r[i]
			p[i].flags = 4 
			p[i].value = 0
			p[i].freeze = false
			gg.addListItems(p)
			gg.setValues(p)
			gg.addListItems(p)
	end	
gg.clearResults()

gg.clearResults()
r = {"-100"}
gg.setRanges(gg.REGION_JAVA_HEAP)
gg.searchNumber("20008" , gg.TYPE_DWORD)
gg.refineNumber("20008", gg.TYPE_DWORD)
q = gg.getResults(1)

for i = 1,#r do
            p = {}
			p[i] = {}
		    p[i].address = q[1].address + r[i]
			p[i].flags = 4 
			p[i].value = 4492
			p[i].freeze = true
			gg.setValues(p)
	end	
		gg.clearResults()

gg.clearResults()
r = {"-100"}
gg.setRanges(gg.REGION_JAVA_HEAP)
gg.searchNumber("20008" , gg.TYPE_DWORD)
gg.refineNumber("20008", gg.TYPE_DWORD)
