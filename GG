end

function LR()
gg.setRanges(gg.REGION_JAVA_HEAP)
gg.searchNumber("16;0;-1;0::30", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("-1;0::30", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
end

function LR1()
gg.setRanges(gg.REGION_JAVA_HEAP)
gg.searchNumber("0", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
t = gg.getResults(30) -- load items
t[1].value = '1'
t[1].freeze = true
gg.clearResults()	
gg.toast("Success")
end

