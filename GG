function WC5()
A = gg.choice({"ᴇᴀsᴛᴇʀ 🐣" ,"ʀᴜᴅᴏʟғ🐏" ,"sᴛᴀʀᴍᴜɴᴄʜᴇʀ🐑"," ❌Clear-Exit","ʙᴀᴄᴋ"},nil," ⚠️ɢᴏ ᴛᴏ ᴇᴀsᴛᴇʀ ᴍᴀᴘ ᴏɴ ᴛʜᴇ 1sᴛ ғʟᴏᴏʀ ᴀɴᴅ ʀᴜɴ ʙᴀᴄᴋ ᴀɴᴅ ʀᴜɴ ᴛʜᴇ sᴄʀɪᴘᴛ.⚠️ ")
if A == 1 then EA() end
if A == 2 then RU() end
