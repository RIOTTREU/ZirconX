function MN3()
A = gg.choice({"‡πÅ‡∏Å‡πâ", "üåä‡∏Å ‡∏•‡∏± ‡∏ö"}, nil, "‚ö†Ô∏è‡∏Ñ‡∏≥‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô‚ö†Ô∏è\n‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏≥‡∏≠‡∏¢‡πà‡∏≤‡∏á‡πÅ‡∏£‡∏Å\n‡πÅ‡∏Å‡πâ‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡πÅ‡∏°‡∏û‡∏≠‡∏∑‡πà‡∏ô‡πÜ\n‡∏ó‡∏µ‡πà‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡πÅ‡∏•‡πâ‡∏ß‡∏Å‡∏•‡∏±‡∏ö‡∏°‡∏≤‡πÅ‡∏•‡πâ‡∏ß‡∏à‡∏≠‡∏î‡∏≥üéà\n(‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å‡∏Å‡∏î‡πÅ‡∏Å‡πâ‡πÅ‡∏•‡πâ‡∏ß)\n‡πÉ‡∏´‡πâ‡∏ß‡∏≤‡∏õ‡∏≠‡∏∞‡πÑ‡∏£‡∏Å‡πá‡πÑ‡∏î‡πâ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏£‡∏µ‡πÅ‡∏ú‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏µ‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏áüéé")
if A == 1 then UNBACK() end
if A == 2 then HOME() end
HOMEDM=-1 
end

function UNBACK()
gg.clearResults()
r = {"-104"}
gg.setRanges(gg.REGION_JAVA_HEAP)
gg.searchNumber("11001" , gg.TYPE_DWORD)
gg.refineNumber("11001", gg.TYPE_DWORD)
q = gg.getResults(1)

for i = 1,#r do
            p = {}
			p[i] = {}
		    p[i].address = q[1].address + r[i]
			p[i].flags = 4 
			p[i].value = 0
			p[i].freeze = false
			gg.addListItems(p)
			gg.setValues(p)
			gg.addListItems(p)
	end	
gg.clearResults()
end

------------------------------ ‡∏≠ ‡∏≠ ‡∏Å ----------------------------------------
function EXIT()
print("‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢Ä‚†§‚†í‚†à‚†â‚†â‚†â‚†â‚†í‚†Ä‚†Ä‚†§‚£Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä\n‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚£∞‚†Å‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢Ä‚£Ñ‚†Ä‚†Ä‚†Ä‚†Ä‚†ë‚°Ñ‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä\n‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†∞‚†ø‚†ø‚†ø‚†£‚£∂‚£ø‚°è‚£∂‚£ø‚£ø‚†∑‚†∂‚†Ü‚†Ä‚†Ä‚†ò‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä\n‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚††‚†¥‚°Ö‚†Ä‚†Ä‚††‚¢∂‚£ø‚£ø‚£∑‚°Ñ‚£Ä‚°Ä‚°Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚°á‚†Ä‚†Ä‚†Ä‚†Ä\n‚†Ä‚£∞‚°∂‚£¶‚†Ä‚†Ä‚†Ä‚°∞‚†Ä‚†Ä‚†∏‚†ü‚¢∏‚£ø‚£ø‚£∑‚°Ü‚†¢‚£â‚¢Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä\n‚†Ä‚¢π‚£ß‚£ø‚£á‚†Ä‚†Ä‚°á‚†Ä‚¢†‚£∑‚£≤‚£∫‚£ø‚£ø‚£á‚†§‚£§‚£ø‚£ø‚†Ä‚¢∏‚†Ä‚£§‚£∂‚†¶‚†Ä‚†Ä‚†Ä‚†Ä\n‚†Ä‚†Ä‚†ô‚¢ø‚£ø‚£¶‚°Ä‚¢á‚†Ä‚†∏‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚†á‚†Ä‚°ú‚£æ‚£ø‚°É‚†á‚¢Ä‚£§‚°Ä‚†Ä\n‚†Ä‚†Ä‚†Ä‚†Ä‚†ô‚¢ø‚£ø‚£Æ‚°Ü‚†Ä‚†ô‚†ø‚£ø‚£ø‚£æ‚£ø‚°ø‚°ø‚†ã‚¢Ä‚†û‚¢Ä‚£ø‚£ø‚£ø‚£ø‚£ø‚°ü‚†Å‚†Ä\n‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†õ‚¢ø‚†á‚£∂‚£§‚£Ñ‚¢Ä‚£∞‚£∑‚£∂‚£ø‚†Å‚°∞‚¢É‚£¥‚£ø‚°ø‚¢ã‚†è‚†â‚†Å‚†Ä‚†Ä‚†Ä\n‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚††‚¢æ‚£ø‚£ø‚£ø‚£û‚†ø‚£ø‚£ø‚¢ø‚¢∏‚£∑‚£å‚†õ‚†ã‚†Ä‚†ò‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä\n‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†ô‚†ô‚£ø‚£ø‚£ø‚£∂‚£∂‚£ø‚£Ø‚£ø‚£ø‚£ø‚£Ü‚†Ä‚†á‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä\n‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢Ä‚£ø‚£ø‚£ø‚£ø‚£ø‚£Ø‚°ô‚¢ø‚£ø‚£ø‚†ü‚°Å‚†∞‚°Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä\n‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚£∏‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚°ü‚†à‚¢©‚£•‚£æ‚£∑‚†ê‚°å‚†ô‚†É‚†Ä\n[‡∏ö‡∏≤‡∏¢ ‡∏ö‡πà‡∏≤‡∏¢üôã]\n‡πÄ‡∏ß‡∏≠‡∏£‡πå‡∏ä‡∏±‡πà‡∏ô‡∏≠‡∏≠‡∏ô‡πÑ‡∏•‡∏ô‡πåüíå")
  gg.skipRestoreState()
  gg.setVisible(true)
  os.exit()
end
while true do
  if gg.isVisible(true) then
    PUBGMH = 1
    gg.setVisible(false)
  end
  if PUBGMH == 1 then
    HOME()
  end
end
