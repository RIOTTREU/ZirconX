 function LLL1() -- npc 
  gg.setRanges(gg.REGION_JAVA_HEAP)
  gg.searchNumber("128;1;3;4::13", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
  gg.refineNumber("3", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
revert = gg.getResults(100, nil, nil, nil, nil, nil, nil, nil, nil)
  gg.editAll("9", gg.TYPE_DWORD)
  gg.clearResults()
end

function RD1() -- npc 
  gg.setRanges(gg.REGION_JAVA_HEAP)
  gg.searchNumber("128;2::5", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
  gg.refineNumber("2", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
revert = gg.getResults(100, nil, nil, nil, nil, nil, nil, nil, nil)
  gg.editAll("5", gg.TYPE_DWORD)
  gg.clearResults()
end


