gg.sleep(600)
gg.setVisible(true)
PUBGMH = -1
function HOME() --Home Menu
MENU = gg.choice({
           "üìÇ ‚ïö…¢·¥è·¥è…¢ ü·¥á ·¥ç·¥Ä·¥òüåé",
           "üìÇ ‚ïö·¥Ñ·¥è·¥ç·¥ò·¥ès…™·¥õ…™·¥è…¥ üö∂",
           "üìÇ ‚ïö·¥°·¥Ä…™·¥õ “ì·¥è Ä ·¥ú·¥ò·¥Ö·¥Ä·¥õ·¥á·¥Ö ·¥Ñ·¥è·¥ç…™…¥…¢ s·¥è·¥è…¥ üçë",
           "‚ùé ·¥á x …™ ·¥õ ‚ùé"
}, nil, "‚ïì ·¥Ñ·¥Ä…¥ ·¥Ñ·¥è…¥·¥õ·¥Ä·¥Ñ·¥õ ·¥°…™·¥õ ú “ì ôüíØ\n‚ïü “ì ô:s·¥Ä ú·¥Ä·¥ò ú·¥ú·¥ç ·¥õ ú·¥Ä ô·¥õ ú·¥Ä·¥°·¥á·¥áüåà\n‚ïô “ì ô:…¥·¥Ä Ä·¥ú·¥Ö·¥á·¥Ñ ú  ô·¥Ä…¥·¥ò·¥Ä…¥·¥á·¥ú·¥ãüëë")
    if MENU == nil then
  else
    if MENU == 1 then
      MN1()
    end
    if MENU == 2 then
     OPEN()
    end
    if MENU == 3 then
     HOME() --‡πÄ‡∏õ‡∏•‡∏∑‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô MN3
    end
    if MENU == 4 then
      EXIT()
    end
  end
  PUBGMH = -1
end



--------STEP[2] Wall & Colourüëá--------

function MN1() --Wall & Colour Menu
WC = gg.multiChoice({
           "‚ïö·¥ú…¥…™·¥è…¥ s·¥Ä·¥çüç´",
           "‚ïö·¥Ä…¥·¥è·¥õ ú·¥á Ä ·¥°·¥è Ä ü·¥Öüåâ",
           "‚ïö·¥Ö·¥è·¥ú…™…¥ ·¥á·¥Ä·¥¢ èüêÜ",
           "‚ïö·¥ç·¥Ä·¥Ö·¥è…¥·¥Äüêç",
           "‚ïö·¥Ö·¥á·¥á Äüêê",
           " ô·¥Ä·¥Ñ·¥ã"
}, nil, "‚îÄ‚îÄ‚ñí‚ñí‚ñí‚ñí‚ñí‚îÄ‚îÄ‚îÄ‚îÄ‚ñÑ‚ñà‚ñà‚ñà‚ñà‚ñÑ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ\n‚îÄ‚ñí‚îÄ‚ñÑ‚ñí‚îÄ‚ñÑ‚ñí‚îÄ‚îÄ‚ñà‚ñà‚ñà‚ñÑ‚ñà‚ñÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ\n‚îÄ‚ñí‚ñí‚ñí‚ñí‚ñí‚ñí‚ñí‚îÄ‚ñê‚ñà‚ñà‚ñà‚ñà‚îÄ‚îÄ‚ñà‚îÄ‚îÄ‚ñà‚îÄ‚îÄ\n‚îÄ‚ñí‚ñí‚ñí‚ñí‚ñí‚ñí‚ñí‚îÄ‚îÄ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñÑ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ\n‚îÄ‚ñí‚îÄ‚ñí‚îÄ‚ñí‚îÄ‚ñí‚îÄ‚îÄ‚îÄ‚ñÄ‚ñà‚ñà‚ñà‚ñà‚ñÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ")
  if WC == nil then
  else
    if WC [1] == true then
      WC1()
    end
    if WC [2] == true then
     WC2()
    end
    if WC [3] == true then
     WC3()
    end
    if WC [4] == true then
     WC4()
    end
    if WC [5] == true then
      WC5()
    end
    if WC [6] == true then
      HOME()
    end
  end
  PUBGMH = -1
end

function WC1()  ----- ‡∏Ç‡πâ‡∏≠‡∏´‡∏±‡∏ß1 ‡∏ö‡∏≠‡∏™ uni Sam Harris-- ------
  A = gg.choice({"·¥°·¥Ä Ä·¥ò", " ô·¥Ä·¥Ñ·¥ã"}, nil, "‚ö†Ô∏è ·¥°·¥Ä Ä…¥…™…¥…¢ ‚ö†Ô∏è\n ú·¥è·¥° ·¥õ·¥è ·¥Ö·¥è …™·¥õ “ì…™ Äs·¥õ\n[·¥Ö·¥è …¥·¥è·¥õ ·¥ò Ä·¥áss ·¥°·¥Ä ü·¥ãüèÉ]\n·¥ús·¥á ·¥õ ú·¥á ·¥°·¥Ä Ä·¥ò ·¥õ·¥è s·¥è“ì…™·¥Ä “ì…™ Äs·¥õ.\n(·¥õ ú·¥á…¥ ·¥ò Ä·¥áss ·¥°·¥Ä Ä·¥ò)\n‚ïî‚ï¶‚ï¶\n‚ï†‚ï¨‚ï¨‚ï¨‚ï£\n‚ï†‚ï¨‚ï¨‚ï¨‚ï£ I üç´\n‚ï†‚ï¨‚ï¨‚ï¨‚ï£ ·¥Ñ ú·¥è·¥Ñ·¥è ü·¥Ä·¥õ·¥á\n‚ïö‚ï©‚ï©‚ï©‚ïù")
if A == 1 then LLL1() gg.sleep(1000) LLL2() gg.sleep(300) xyz3() LLL3() gg.sleep(2300) xyz44() xyz45() end
if A == 2 then MN1() end
HOMEDM=-1 
end

 function LLL1() -- npc 
  gg.setRanges(gg.REGION_JAVA_HEAP)
  gg.searchNumber("128;1;3;4::13", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
  gg.refineNumber("3", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
revert = gg.getResults(100, nil, nil, nil, nil, nil, nil, nil, nil)
  gg.editAll("9", gg.TYPE_DWORD)
  gg.clearResults()
end

function LLL2() -- ‡πÅ‡∏°‡∏û
gg.clearResults()
r = {"-104"}
gg.setRanges(gg.REGION_JAVA_HEAP)
gg.searchNumber("20008" , gg.TYPE_DWORD)
gg.refineNumber("20008", gg.TYPE_DWORD)
q = gg.getResults(1)

for i = 1,#r do
            p = {}
			p[i] = {}
		    p[i].address = q[1].address + r[i]
			p[i].flags = 4 
			p[i].value = 0
			p[i].freeze = false
			gg.addListItems(p)
			gg.setValues(p)
			gg.addListItems(p)
	end	
gg.clearResults()

gg.clearResults()
r = {"-100"}
gg.setRanges(gg.REGION_JAVA_HEAP)
