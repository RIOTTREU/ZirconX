function MN2() --üìÇ Menu Lobby
MG = gg.multiChoice({"Remove delay‚ôª" ,"Death burnüëª" ,"Remove attack gesturesüî™" ,"Through the mapüóø" ,"Far skillsüéø" ,"Duck stoneüöÑ","Movement speedüèÉ","üéíBACKING"},nil,"‚ö†Ô∏èThis script is made for vmos version 5.1‚ö†Ô∏è")
  if MG == nil then
  else 
    if MG [1] == true then
      DD()
    end
    if MG [2] == true then
     TFAR1()
    end
    if MG [3] == true then
     ATK()
    end
    if MG [4] == true then
     MAP()
    end
    if MG [5] == true then
      FAR()
    end
    if MG [6] == true then 
      DOWN()
    end
    if MG [7] == true then
      RUN()
      end
    if MG [8] == true then
      HOME()
    end
    end
  PUBGMH = -1
end

--Delay--
function DD()
B = gg.choice({"Open" ,"Close","üéíBACKING"},nil,"üöÄRemove delay and auto skip‚ôª")
if B == 1 then D1() AUTO1() end
if B == 2 then D2() end
if B == 3 then MN2() end
end

function AUTO1() 
gg.setRanges(gg.REGION_JAVA_HEAP)
gg.searchNumber("11,001;2::305", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("2", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)

revert = gg.getResults(100, nil, nil, nil, nil, nil, nil, nil, nil)
local t = gg.getResults(100, nil, nil, nil, nil, nil, nil, nil, nil)
for i, v in ipairs(t) do
	if v.flags == gg.TYPE_DWORD then
		v.value = "1"
		v.freeze = true
	end
end
gg.addListItems(t)
t = nil
gg.clearResults()
gg.setVisible(true)
gg.toast('Success')
end

function D1()
gg.searchNumber("2000;0;0;0;1~1000F::65", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("0", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
revert = gg.getResults(6, nil, nil, nil, nil, nil, nil, nil, nil)
local t = gg.getResults(6, nil, nil, nil, nil, nil, nil, nil, nil)
for i, v in ipairs(t) do
	if v.flags == gg.TYPE_DWORD then
		v.value = "0"
		v.freeze = true
	end
end
gg.addListItems(t)
t = nil

gg.clearResults()
gg.toast("ON")
end

