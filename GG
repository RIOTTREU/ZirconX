gg.setVisible(false)
gg.sleep(350)
gg.toast("\n‚ñà‚ñà‚ñì‚ñí¬≠‚ñë ‚ñ∫‚ñ¨ WELCOME ‚ñ¨‚óÑ ‚ñë‚ñí‚ñì‚ñà‚ñà")
gg.sleep(600)
gg.setVisible(true)
PUBGMH = -1
function HOME() --Home Menu
MENU = gg.choice({
           "üìÇ Google map üåé",
           "üìÇ Composition üö∂",
--           "üìÇ ‚ïö·¥ú·¥ò·¥Ö·¥Ä·¥õ·¥á ·¥ò ü·¥Ä è ü…™s·¥õ üìÖ (üÜï)",
           "‚ùé E X I T ‚ùé"
}, nil, "üî•Can connect to facebooküî•\nüåúFB:Sahaphum thabthaweeüåõ\n")
    if MENU == nil then
  else
    if MENU == 1 then
      MN1()
    end
    if MENU == 2 then
     OPEN()
    end
--    if MENU == 3 then
--     UAT() --‡πÄ‡∏õ‡∏•‡∏∑‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô MN3
--    end
    if MENU == 3 then
      EXIT()
    end
  end
  PUBGMH = -1
end


